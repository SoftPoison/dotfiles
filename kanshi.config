profile mobile {
    output eDP-1 enable scale 2
}

profile work {
    output eDP-1 disable
    output 'XMI Mi Monitor Unknown' position 0,0 mode 3440x1440@144
}

profile home {
    output eDP-1 disable
    output 'ViewSonic Corporation VX2718-2KPC WAL212101486' position 0,0 mode 2560x1440@59.951
}

profile meetings {
    output eDP-1 enable scale 2 position 0,1080
    output 'Samsung Electric Company SAMSUNG 0x00000F00' position 0,0 mode 1920x1080@60
}
